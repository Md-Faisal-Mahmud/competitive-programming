//link :  https://www.urionlinejudge.com.br/judge/en/problems/view/1306

#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ll long long int

using namespace std;
//using ll = long long int;

int main()
{

    faster
    int r=1, n=1, cnt = 0;
    while (r != 0 && n!=0)
    {
        cin >> r >> n ;
        if (r!=0 && n!=0)
        {
            cnt++;
            int i = 0;
            int result = n + (n*26);

            if (result < r)
                cout << "Case " << cnt << ": " << "impossible" << endl ;
            else
            {
                if (r<=n)
                {
                    cout  << "Case " << cnt << ": " << i << endl ;

                }
                else
                {
                    i++;
                    int z = n+(n*i);
                    while (z < r)
                    {
                        i++;
                        z = n+(n*i) ;
                    }
                    cout  << "Case " << cnt << ": " << i << endl ;
                }
            }
        }
    }
}
