//link : https://www.urionlinejudge.com.br/judge/en/problems/view/1306

#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ll long long int
using namespace std;

int main()
{

    faster
    int cnt = 0;
    for (int i=0; i<10002 ; i++)
    {

        int r, n; cin >> r >> n ;
        if (r==0 && n==0)
        {
            break;
        }
        else
        {
            cnt++;
            int i = 0;
            int result = n + (n*26);

            if (result < r)
                cout << "Case " << cnt << ": " << "impossible" << endl ;
            else
            {
                if (r<=n)
                {
                    cout  << "Case " << cnt << ": " << i << endl ;

                }
                else
                {
                    i++;
                    int z = n+(n*i);
                    while (z < r)
                    {
                        i++;
                        z = n+(n*i) ;
                    }
                    cout  << "Case " << cnt << ": " << i << endl ;
                }
            }
        }
    }

}
