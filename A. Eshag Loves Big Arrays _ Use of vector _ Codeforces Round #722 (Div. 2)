Link : https://codeforces.com/contest/1529/problem/A

#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
using namespace std;

int main ()
{
    faster
    
    int t ;
    cin >> t ;
    while (t--)
    {
        int sum = 0, cnt = 0;
        int n ;
        cin >> n ;
        vector <int> v(n);
        for (int &x : v)
        {
            cin >> x ;
        }

        int min = *min_element(v.begin() , v.end());

        for (int &x : v)
        {
            if (x > min)
                cnt ++;
        }

        cout  << cnt << endl ;
    }
}
