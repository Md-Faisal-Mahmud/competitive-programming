#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
#define ll long long int
using namespace std;

int main()
{
    faster
    ll a,sum=0;
    cin >> a;
    if (a>0)
    {
        for (int i=1; i<=a ; i++)
        {
            sum = sum + i*i;
        }
        cout << sum;
    }
    else
        cout <<"No Square";

}
