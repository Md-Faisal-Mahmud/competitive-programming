#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
using namespace std;

int pow(int base,int power)
{
    if(power==0) return 1;
    if(power==1) return base;

    int answer;
    if(power%2==0)
        answer = pow(base,power/2) * pow(base,power/2);
    else
        answer = base * pow(base,(power-1)/2) * pow(base,(power-1)/2);

    return answer;


}
int main()
{
    faster
    int base,power ;  cin >> base >> power;
    
    int ans = pow(base,power);
    cout << ans;

}



