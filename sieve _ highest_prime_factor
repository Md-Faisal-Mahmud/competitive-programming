#include<bits/stdc++.h>
#define faster ios::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
using namespace std;
const int mod=1000000007;

#define size 1000000  // 10^6  _ 1 million
int arr[size];

void sieve(int n) {
    for (int i=2; i<=n; i++){
        if(arr[i] == 0){
            arr[i] = i;
        }

        int j = 2;

        while(i*j<=n){
            arr[i*j] = i;
            j++;
        }
    }
}

int main()
{
    faster
    int n; cin >> n;

    sieve(n);

    for (int i=2; i<=n; i++) {
            cout << arr[i] << " ";
    }
}
